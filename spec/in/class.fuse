using class;

-- basic class --

new Example {
	__init(a = 5)->
		@a = a;
	print()->
		print(@a);
}

a = Example();
b = Example(15);

-- extension class --

new ExampleToo extends Example {
	__init(a, b = 10)->
		@a, @b = a, b;
	print()->
		print(@b);
}

c = ExampleToo();
c:print(); -- @b
c:print<Example>(); -- @a

-- superinitialization --

new ExampleThree extends ExampleToo {
	__init(a, b)->
		self:__init<ExampleToo>(a, b);
}
