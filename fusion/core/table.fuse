local _table = {};

for (k, v in pairs(table)) {
    _table[k] = v;
}

_table.join(tNative, tInput)-> {
    for (k, v in pairs(tInput))
        tNative[k] = v;
}

_table.copy(tInput)-> {
    local tOutput = {};
    for (k, v in pairs(tInput))
        tOutput[k] = v;
